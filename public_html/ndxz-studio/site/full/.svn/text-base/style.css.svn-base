/* ----------------------- */
/* Miniature Slideshow     */
/* ----------------------- */
	
.miniature-nav-contener ul {
	white-space: nowrap;
	padding: 0;
	margin: 0;
	height: 44px;
	width: 1600px;
}	
.minature-nav li {
	float: left;
	list-style-type: none;
	margin-right: 12px;
}	

.miniature-nav-contener {
	width: 602px;
	height: 55px;
	overflow: hidden;
	position: relative;
	margin-top: 10px;
	z-index: 60;
}


.miniature-nav-suivante {
	display: none;
	position: absolute;
	margin: -17px 0 0 760px;
	width: 15px;
	height: 10px;
	z-index: 250;		
}


.miniature-nav-suivante:hover {
	background-color: white;
	text-decoration: none;
}

.miniature-nav-precedente {
	display: none;
	position: absolute;
	margin: -17px 0 0 760px;
	width: 22px;
	height: 10px;
	z-index: 250;	
}

.miniature-nav-precedente:hover {
	background-color: white;
	text-decoration: none;
}

.minature-nav img { 
	margin: 0 0 0 0;
}

.minature-nav a {
	display: block;
}


.minature-nav a.activeSlide {
	background: none;
	border-bottom: 1px solid black;
}

.minature-nav a:hover {
	background: none;
	border-bottom: 1px solid black;

}

/* class pour application du scroll */
.scrollable {
	overflow: auto;
}